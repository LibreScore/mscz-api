{
  "name": "mscz-api",
  "version": "0.5.1",
  "description": "HTTP API to manage MSCZ files.",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LibreScore/mscz-api.git"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "eslint": "eslint",
    "start": "node dist/index.js",
    "stop": "fuser -s -k -INT 3000/tcp",
    "deploy": "npm run build && gcloud functions deploy api --runtime nodejs12 --trigger-http --entry-point default --project mscz-api --memory=512M --region=us-west4 --allow-unauthenticated"
  },
  "author": "Librescore",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/LibreScore/mscz-api/issues"
  },
  "homepage": "https://github.com/LibreScore/mscz-api#readme",
  "dependencies": {
    "@librescore/fonts": "^0.2.1",
    "@librescore/sf3": "^0.3.0",
    "@vendia/serverless-express": "^3.4.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.2.3",
    "nconf": "^0.11.0",
    "ts-custom-error": "^3.2.0",
    "webmscore": "^0.15.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/content-type": "^1.1.3",
    "@types/express": "^4.17.9",
    "@types/express-rate-limit": "^5.1.0",
    "@types/nconf": "^0.10.0",
    "@types/node": "^14.14.12",
    "@types/raw-body": "^2.3.0",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "eslint": "^7.15.0",
    "typescript": "^4.1.3"
  }
}